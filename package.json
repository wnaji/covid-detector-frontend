{
  "name": "pfe-web-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build --configuration dev --outputPath=dist/pfe-web-frontend-dev && ng build --configuration production --outputPath=dist/pfe-web-frontend-prod",
    "start": "node server.js",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "ng serve",
    "build:workflow": "ng build --configuration=dev",
    "build:ci": "npm run test && npm run build:workflow"
  },
